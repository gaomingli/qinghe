/* custom-tab-bar/index.wxss */
.tabbar_box{
  width: 100%;
  height: 110rpx;
  display: flex;
  align-items: center;
  font-size:22rpx;
  color: #2B2B2B;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #FFFFFF;
  border-top: 1rpx solid #d7d7d7;
}
.tabbar_box .tabbar_item{
  width: 20%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tabbar_item image{
  width: 42rpx;
  height: 40rpx;
  margin-bottom: 10rpx;
}
.tabbar_item:nth-child(3) image{
  width: 80rpx;
  height: 80rpx;
  position: relative;
  top: -12rpx;
}
.tabbar_item:nth-child(3) text{
  position: relative;
  top: -12rpx;
}