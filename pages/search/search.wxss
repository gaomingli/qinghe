page {
  background: #ffffff;
}

/*搜索部分*/
.searchBox {
  margin: 25rpx 30rpx 25rpx 30rpx;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.searchBox_input {
  display: flex;
  /* width: 690rpx; */
  width: 500rpx;
  height: 65rpx;
  background: rgba(248, 248, 248, 1);
  border-radius: 33px;
  align-items: center;
  padding-left: 46rpx;
}
.btnView{
  width: 120rpx;
  height: 65rpx;
  background:#53B2AA;
  line-height: 65rpx;
  text-align: center;
  color: #ffffff;
  border-radius: 33rpx;

}


.searchBox_input image {
  width: 27rpx;
  height: 29rpx;
  margin-right: 20rpx;
}

.searchBox_input input {
  font-size: 28rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.module{
  width: 750rpx;
  margin: auto;
  position: relative;
  display: flex;
justify-content: space-between;
padding: 30rpx;
box-sizing: border-box;
align-items: center;
font-size: 28rpx;
color: #333333;
}
.module::after{
  display: block;
  position: absolute;
  content: "";
  width: 750rpx;
  height: 5rpx;
  background: #f8f8f8;
  bottom: 0;
}
.module .text{
  width: 500rpx;
  display: -webkit-box;
          -webkit-line-clamp: 2;
          -webkit-box-orient: vertical;
          overflow: hidden;
          text-overflow: ellipsis;
          line-height: 45rpx;
}
.watch{
   background:#53B2AA;
   width: 130rpx;
   height: 60rpx;
   line-height: 60rpx;
   text-align: center;
   border-radius: 30rpx;
color: #ffffff;
font-size: 22rpx;


}


