/* pages/my/my.wxss */
@import "../index/index.wxss";
.my{
  background-color: #f8f8f8;
  position: relative;
}
.my .my_bg{
  background: #fff;
  height: 362rpx;
}
.index_menu navigator image{
  width: 60rpx;
  height: 50rpx
}
.index_menu .menu_text{
  color: #333;
  font-size: 24rpx
}
.my_order{
 width:95%;
margin-left:auto;
margin-right:auto;
margin-top:-60rpx;
border-radius:16rpx;
border:1px solid #fff;
background:#fff;
z-index:999;
position:relative;
}
.my_order .order{
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 30rpx;
  color: #2B2B2B;
  font-size: 32rpx;
  border-bottom: 1px solid #E5E5E5;
}
.my_bg image{
  width: 100%;
  height: 100%
}
.my .part{
  width: 94%;
  margin: 20rpx auto;
  border-radius:16rpx;
border:1px solid #fff;
}
.personalinfo{
 display:flex;
justify-content:space-between;
padding-left:60rpx;
padding-right:60rpx;
align-items:center;
position:absolute;
top:100rpx;
left:0;
right:0;
}
.left_part{
  display: flex;
  justify-content: space-between;
}
.one image{
  width: 100rpx;
  height: 100rpx;  
  vertical-align: middle;
} 

.orderImg{
  width: 70rpx;
  height: 60rpx;
}


.two{
  margin-left: 27rpx;
}
.two .name{
color: #ffffff;
font-size: 32rpx;
}
.two .num{
  color: #ffffff;
  font-size: 20rpx;
  margin-top: 23rpx;
}
.two .num text{
  font-size: 40rpx;
}
.three{
  margin-left: 16rpx;
  background-color: rgba(255, 255, 255, 0.3);
  color: #fff;
  text-align: center;
  line-height: 48rpx;
  height: 48rpx;
  width: 100rpx;
  border-radius: 24rpx;
  font-size: 24rpx;
}
.right_part image{
  width: 14rpx;
  height: 26rpx;
}
.index_menu{
  justify-content: space-around;
}